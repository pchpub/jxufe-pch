			function appendGrayCssFile(fn, fid){
				var el;
				var ft='css';
				if (ft=="css"){
					el=document.createElement("link");
					el.setAttribute("id",fid);
					el.setAttribute("rel", "stylesheet");
					el.setAttribute("type", "text/css");
					el.setAttribute("href", fn);
				}
				document.getElementsByTagName("head")[0].appendChild(el);
			}

			var myapps = "" ;

			var xt_allgraytheme = "0";
			if ("1" == xt_allgraytheme) {
			    var fid = "";
			    var fn = "/frame14/menu/myapps.gray.css?t=60352170399037362123894"
				appendGrayCssFile(fn, fid);
			}

		</script>
		
